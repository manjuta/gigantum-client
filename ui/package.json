  "scripts": {
    "start": "node scripts/start.js NODE_ENV='development'",
    "start:local": "BUILD_TYPE='local' node scripts/start.js NODE_ENV='development'",
    "start:cloud": "BUILD_TYPE='cloud' node scripts/start.js NODE_ENV='development'",
    "build": "node scripts/build.js",
    "build:local": "BUILD_TYPE='local' node scripts/build.js",
    "build:cloud": "BUILD_TYPE='cloud' node scripts/build.js",
    "build:local:prod": "BUILD_TYPE='local' BUILD_DIST='prod' node scripts/build.js",
    "build:cloud:prod": "BUILD_TYPE='cloud' BUILD_DIST='prod' node scripts/build.js",
    "relay": "relay-compiler --watchman false --src ./src --schema ./schema.graphql",
    "relay-test": "relay-compiler --src ./__tests__ --schema ./schema.graphql",
    "update-schema": "graphql codegen -p labbook",
    "update-json-schema": "babel-node ./scripts/updateSchema.js",
    "build-tests": "BABEL_ENV='development' babel-node ./scripts/buildTests.js",
    "mock": "node __mocks__/react-relay.js",
    "polyfills": "babel-node ./config/polyfills.js",
    "lint-fix": "eslint ./.eslintrc --fix ./src",
    "rebuild-sass": "npm rebuild node-sass --force",
    "generate-icons": "babel-node ./scripts/generateIcons.js",
    "update-globals": "npm install -g @babel/cli@7.2.3 @babel/core@7.2.2 jest@23.6.0 relay-compiler@2.0.0",
    "__comment-test__": "********************** TESTS SCRIPTS GO HERE **********************",
    "test": "BABEL_ENV=test TZ='Europe/London' jest src __tests__",
    "test:verbose": "BABEL_ENV=test TZ='Europe/London' jest --verbose src",
    "test:update": "TZ='Europe/London' jest src -u",
    "test:proxy": "USE_PROXY=true jest",
    "test:output": "jest src --json --outputFile=results/.jest-testresults.json",
    "test:output:watch": "yarn test:output -- --watch",
    "__comment-storybook__": "********************** STORYBOOK GOES HERE **********************",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook"
  },
  "eslint": {
    "plugins": [
      "import",
      "jsx-a11y",
      "react"
    ]
  }
